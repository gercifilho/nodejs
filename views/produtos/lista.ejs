<html>
    <body>
    <h2 class="basic-title">Listagem de livros</h2>
    <table class="table table-condensed table-bordered
    table-striped table-hover">
    	<thead>
    		<tr>
    			<td>Titulo</td>
    			<td>Preco</td>
    			<td>Descricao</td>
    		</tr>
    	</thead>
    	<tbody>
    		<% for(var i=0; i<lista.length; i++) {%>
    		<tr>
    			<td><%= lista[i].titulo %></td>
    			<td><%= lista[i].preco %></td>
    			<td><%= lista[i].descricao %></td>
    		</tr>
    		<% } %>
    	</tbody>
    </table>
    </body>
</html>